{
    "name": "@nmshd/connector",
    "version": "2.0.7",
    "private": true,
    "description": "The Enmeshed Business Connector",
    "license": "MIT",
    "author": "j&s-soft GmbH",
    "scripts": {
        "lint": "npm run lint:tsc && npm run lint:prettier && npm run lint:eslint",
        "lint:prettier": "prettier --check ./src ./test ./packages/*/src",
        "lint:eslint": "eslint --ext .ts ./src ./test ./packages/*/src",
        "lint:tsc": "lerna run build:ci && tsc --noEmit && tsc -p test/tsconfig.json --noEmit",
        "build": "run-p tsc copy-additional-files",
        "build:watch": "run-p -r tsc:watch copy-additional-files:watch",
        "copy-additional-files": "npx cpx \"src/**/*.{json,yml}\" dist",
        "copy-additional-files:watch": "npx cpx \"src/**/*.{json,yml}\" dist --watch",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "test:ci": "jest -i",
        "test:local": ". scripts/testSetup.sh && jest",
        "test:watch": ". scripts/testSetup.sh && jest --watch"
    },
    "jest": {
        "maxWorkers": 2,
        "preset": "ts-jest",
        "setupFilesAfterEnv": [
            "@alex_neo/jest-expect-message"
        ],
        "testEnvironment": "node",
        "testTimeout": 60000
    },
    "dependencies": {
        "@js-soft/docdb-access-mongo": "1.0.2",
        "@js-soft/node-logger": "1.0.0",
        "@js-soft/ts-utils": "^1.0.1",
        "@nmshd/runtime": "1.0.5",
        "agentkeepalive": "4.1.4",
        "axios": "0.24.0",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "express": "4.17.1",
        "helmet": "4.6.0",
        "json-stringify-safe": "5.0.1",
        "jsonschema": "1.4.0",
        "nconf": "0.11.3",
        "on-headers": "1.0.2",
        "rapidoc": "9.1.3",
        "reflect-metadata": "0.1.13",
        "swagger-ui-express": "4.1.6",
        "typescript": "4.4.4",
        "typescript-ioc": "3.2.2",
        "typescript-rest": "3.0.4",
        "typescript-rest-ioc": "1.0.1",
        "yamljs": "0.3.0"
    },
    "devDependencies": {
        "@alex_neo/jest-expect-message": "^1.0.5",
        "@js-soft/eslint-config-ts": "1.4.2",
        "@js-soft/license-check": "1.0.3",
        "@nmshd/connector-sdk": "1.1.4",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "4.17.13",
        "@types/helmet": "0.0.48",
        "@types/jest": "^27.0.2",
        "@types/jest-expect-message": "^1.0.3",
        "@types/jest-json-schema": "^2.1.3",
        "@types/json-stringify-safe": "^5.0.0",
        "@types/luxon": "^2.0.7",
        "@types/nconf": "^0.10.1",
        "@types/node": "^16.11.7",
        "@types/on-headers": "^1.0.0",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/wait-on": "^5.3.1",
        "@types/yamljs": "^0.2.31",
        "eslint": "^8.2.0",
        "get-port": "^5.1.1",
        "jest": "^27.3.1",
        "jest-json-schema": "^5.0.0",
        "lerna": "^4.0.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "wait-on": "^6.0.0"
    }
}
